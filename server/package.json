{
  "name": "hackoholics",
  "version": "1.0.0",
  "description": "service for spark 720 idea",
  "main": "src/index.ts",
  "scripts": {
    "compile": "tsc",
    "compile-production": "tsc --sourceMap false",
    "start-build": "node build/index.js",
    "start": "npm-run-all -l --sequential compile start-build",
    "lint": "tslint -p .",
    "lint-fix": "tslint -p . --fix",
    "prettier-check": "prettier --list-different \"src/**/*.ts\"",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "test": "jest --coverage --verbose -i",
    "verify-ci": "npm-run-all -l --parallel prettier-check lint test compile",
    "verify": "npm-run-all -l --sequential prettier lint-fix test compile"
  },
  "author": "hackoholics",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "http-status": "^1.3.2",
    "hxsb-cache": "^1.3.0",
    "mongodb":"3.5.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/mongodb": "3.5.25",
    "@types/supertest": "^2.0.7",
    "axios-mock-adapter": "^1.16.0",
    "jest": "^24.7.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-blank-line": "0.0.9",
    "typescript": "^3.4.2"
  }
}
